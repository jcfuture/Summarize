<?xml version="1.0" encoding="utf-8" ?>
        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.parking.dao.UserMapper">
    <select id="getAll" resultType="user">
        select * from user
    </select>

    <insert id="enrollInsert" keyProperty="id">
        insert into user (username , password , role) values (#{username} , #{password} , default )
    </insert>

    <select id="loginUser" resultType="user">
        select * from user where username = #{account} and password = #{password}
    </select>
</mapper>