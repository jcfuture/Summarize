<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.parking.dao.VehicleMapper">
    <insert id="insert">
        insert into vehicle (licensePlate, local, illustrate, time)
        values (#{licensePlate} , #{local} , #{illustrate} , now());
    </insert>

    <select id="getByLocal" resultType="Vehicle">
        select * from vehicle where local = #{local}
    </select>
</mapper>